{
  "name": "fullstack-boilerplate",
  "private": true,
  "scripts": {
    "dev": "yarn prisma && next dev",
    "build": "yarn prisma && next build",
    "build:only": "yarn generate && next build",
    "start": "yarn prisma && next start",
    "lint": "yarn generate && next lint --fix",
    "prepare": "husky install",
    "prisma": "yarn generate && npx prisma db push",
    "generate": "npx prisma generate && npx ts-node --transpile-only -P nexus.tsconfig.json pages/api/graphql",
    "docker:up": "docker-compose -f docker-compose.test.yml up -d",
    "docker:wait": "./wait-for-it.sh -h localhost -p 5433 -t 0 && sleep 1",
    "docker:down": "docker-compose -f docker-compose.test.yml down -v",
    "test:reset": "env-cmd -f .env.test npx prisma migrate reset -f",
    "test:jest": "env-cmd -f .env.test jest -i",
    "test:live": "yarn test:reset && yarn generate && env-cmd -f .env.test jest",
    "test": "yarn docker:up && yarn docker:wait && yarn generate && yarn test:reset && yarn test:jest"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@headlessui/react": "^1.4.2",
    "@prisma/client": "^3.14.0",
    "@reduxjs/toolkit": "^1.8.2",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "apollo-server-micro": "^3.5.0",
    "argon2": "^0.28.3",
    "cookie": "^0.4.1",
    "graphql": "^16.5.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-middleware": "^6.1.28",
    "graphql-query-complexity": "^0.11.0",
    "graphql-rate-limit": "^3.3.0",
    "graphql-shield": "^7.5.0",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "12.1.0",
    "nexus": "^1.3.0",
    "nexus-prisma": "0.34.0",
    "postcss-import": "^14.0.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "secure-random-string": "^1.1.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.5",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/cookie": "^0.4.1",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/graphql-query-complexity": "^0.2.1",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "16.11.12",
    "@types/react": "18.0.1",
    "@types/react-redux": "^7.1.24",
    "@types/secure-random-string": "^1.1.1",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.4.5",
    "env-cmd": "^10.1.0",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "husky": "^7.0.0",
    "jest": "^27.4.5",
    "jest-mock-extended": "^2.0.4",
    "postcss": "^8.2.15",
    "prisma": "^3.14.0",
    "react-test-renderer": "^17.0.2",
    "tailwindcss": "^3.0.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.7.3"
  }
}
